<?xml version='1.0' encoding='utf-8'?>
<widget id="io.github.cell303.cashwithfriends" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cash With Friends</name>
    <description>
      A smart debt tracker.
    </description>
    <author email="f.klampfer@gmail.com" href="http://cell303.tumblr.com">
      Florian Klampfer
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />

    <gap:plugin name="com.phonegap.plugins.facebookconnect" version="0.4.0">
      <param name="APP_ID" value="254645454692956" />
      <param name="APP_NAME" value="cashwithfriends" />
    </gap:plugin>
</widget>
